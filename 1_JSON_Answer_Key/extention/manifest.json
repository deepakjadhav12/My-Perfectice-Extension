{
  "manifest_version": 3,
  "name": "1 Myperfectice JSON Answer Key",
  "version": "1.0",
  "description": "Gets the answer key of Myperfectice",
  "permissions": ["tabs", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
